{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "Cashflowpoly.Api",
    "Audience": "Cashflowpoly.Client",
    "ActiveKeyId": "legacy",
    "SigningKeyEnvironmentVariable": "JWT_SIGNING_KEY",
    "SigningKeysJsonEnvironmentVariable": "JWT_SIGNING_KEYS_JSON",
    "AccessTokenMinutes": 480
  },
  "Auth": {
    "AllowPublicInstructorRegistration": true
  },
  "AuthBootstrap": {
    "SeedDefaultUsers": false
  },
  "Networking": {
    "TrustedProxies": []
  },
  "AllowedHosts": "*"
}
